{
  "name": "knowledge-graph-search",
  "version": "1.0.0",
  "description": "Search engine for GOV.UK content",
  "main": "src/backend/server.ts",
  "scripts": {
    "test": "jest --watchAll --coverage --verbose --detectOpenHandles",
    "start": "ts-node src/backend/server.ts",
    "dev": "nodemon src/backend/server.ts",
    "local": "env $(cat .env|xargs) npm run dev",
    "build": "webpack",
    "lint": "eslint './**/*.{ts,tsx}' --fix",
    "check-lint": "eslint './**/*.{ts,tsx}'",
    "format": "prettier --write '.'",
    "check-format": "prettier --check '.'",
    "copy-assets": "node -e \"require('fs').copyFileSync('./node_modules/govuk-frontend/govuk/all.js', './src/public/assets/js/all.js', 0);require('fs').cpSync('./node_modules/govuk-frontend/govuk/assets', './src/public/assets', {recursive: true});\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphagov/knowledge-graph-search.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alphagov/knowledge-graph-search/issues"
  },
  "homepage": "https://github.com/alphagov/knowledge-graph-search#readme",
  "dependencies": {
    "@google-cloud/bigquery": "^6.0.3",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.14",
    "axios": "^1.2.1",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-stringify": "^6.2.4",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "got": "^12.5.0",
    "govuk-frontend": "^4.6.0",
    "ioredis": "^5.3.2",
    "nunjucks": "^3.2.4",
    "passport-oauth2": "^1.7.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.2",
    "@types/nunjucks": "^3.2.2",
    "@types/passport-oauth2": "^1.4.12",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "cypress": "^12.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "ignore-webpack-plugin": "^0.3.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0"
  }
}
